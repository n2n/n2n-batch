{
	"name" : "n2n/n2n-batch",
	"type" : "library",
	"description" : "batch job support for n2n framework",
	"keywords" : [
		"n2n",
		"batch job"
	],
	"homepage" : "https://n2n.rocks/",
	"license" : "LGPL-3.0-or-later",
	"authors" : [
		{
			"name" : "Andreas von Burg",
			"email" : "a@von-burg.net",
			"homepage" : "https://andreas.von-burg.net/"
		}
	],
	"minimum-stability" : "dev",
	"prefer-stable" : true,
	"config" : {
		"vendor-dir" : "src/vendor",
		"allow-plugins": {
			"n2n/n2n-composer-module-installer": true
		}
	},
	"require" : {
		"n2n/n2n-reflection" : "~7.4",
		"n2n/n2n-util" : "~7.4",
        "n2n/n2n": "~7.4"
	},
	"autoload" : {
		"psr-4" : {
			"n2n\\batch\\" : "src/app/n2n/batch"
		}
	},
	"autoload-dev" : {
		"psr-4" : {
			"n2n\\batch\\" : "src/test/n2n/batch"
		}
	},
	"require-dev" : {
		"phpunit/phpunit" : "^9.5"
	}
}
